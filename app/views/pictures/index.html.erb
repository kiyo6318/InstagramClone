<h1>投稿一覧</h1>

<% @pictures.each do |picture| %>
  <div><%= link_to picture.user.name,user_path(picture.user.id) %></div>
  <div>
  <% if logged_in? %>
    <%= link_to image_tag(picture.image.url),picture_path(picture.id) %>
  <% else %>
    <%= image_tag(picture.image.url) %>
  <% end %>
  </div>
  <br>
<% end %>